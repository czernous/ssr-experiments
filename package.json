{
  "name": "ts-node-react-ssr",
  "version": "1.0.0",
  "description": "A simple server side rendering template for React using vanilla Node.js",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format:test": "prettier \"src/**/*.{ts,tsx,html,md,scss,json}\" --list-different",
    "format:write": "prettier \"src/**/*.{ts,tsx,html,md,scss,json}\" --write",
    "lint": "eslint \"src/**/*.{ts,tsx,html,md,scss,json}\" --fix && npm run format:write",
    "hmr:client": "cross-env NODE_ENV=development webpack serve --config webpack.config.client.ts --mode=development",
    "dev:build-client": "cross-env NODE_ENV=development webpack --config webpack.config.client.ts --mode=development -w",
    "dev:build-server": "cross-env NODE_ENV=development webpack --config webpack.config.server.ts --mode=development -w",
    "dev:start": "nodemon ./dist/server.js",
    "dev": "npm-run-all --parallel dev:*",
    "prod:build-client": "cross-env NODE_ENV=production webpack --config webpack.config.client.ts --mode=production",
    "prod:build-server": "cross-env NODE_ENV=production webpack --config webpack.config.server.ts --mode=production -w",
    "prod:start": "nodemon ./dist/server.js",
    "prod": "npm-run-all lint --parallel prod:*"
  },
  "author": "Anton Czernous",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.17.0",
    "@babel/types": "^7.17.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.2",
    "@swc/core": "^1.2.136",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/loadable__webpack-plugin": "^5.7.3",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-helmet": "^6.1.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-manifest-plugin": "^3.0.5",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "@reduxjs/toolkit": "^1.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.3.0"
  }
}
